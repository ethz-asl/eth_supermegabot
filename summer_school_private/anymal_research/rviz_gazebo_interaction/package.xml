<?xml version="1.0"?>

<package format="2">

  <name>rviz_gazebo_interaction</name>
  <version>0.6.0</version>
  <description>Tool to apply wrenches to objects by sending them to gazebo.</description>

  <license>Proprietary</license>

  <author email="lisler@anybotics.com">linus</author>
  <maintainer email="lisler@anybotics.com">linus</maintainer>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>gazebo_msgs</depend>
  <depend>qtbase5-dev</depend>
  <depend>roscpp</depend>
  <depend>rqt_gui</depend>
  <depend>rqt_gui_cpp</depend>
  <depend>rviz</depend>

  <export>
	<rviz plugin="${prefix}/plugin_description.xml"/>
  </export>

</package>
